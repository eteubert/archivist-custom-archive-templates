#, php-format
msgid "Settings for \"%1s\" Template"
msgstr "模板 “%1s” 的设置"

#: /Users/ericteubert/code/archivist-custom-archive-templates/archivist.php:793
msgid "Default Template"
msgstr "默认模板"

#: /Users/ericteubert/code/archivist-custom-archive-templates/archivist.php:799
msgid "Set to Default"
msgstr "设为默认"

#: /Users/ericteubert/code/archivist-custom-archive-templates/archivist.php:818
msgid "Template"
msgstr "模板"

